rt: hit</body><body package="SuffixConditionals-Tests">evaluatesFirstBlockIfConditionIsTrue	&lt;test&gt;	| hit |	hit := nil.	[hit := #first] if: true else: [hit := #second].	self assert: hit == #first</body><body package="SuffixConditionals-Tests">evaluatesSecondBlockIfConditionIsFalse	&lt;test&gt;	| hit |	hit := nil.	[hit := #first] if: false else: [hit := #second].	self assert: hit == #second</body><body package="SuffixConditionals-Tests">ignoresBlockIfConditionIsFalse	&lt;test&gt;	| hit |	hit := false.	[hit := true] if: false.	self deny: hit</body><body package="SuffixConditionals-Tests">ignoresBlockUnlessConditionIsTrue	&lt;test&gt;	| hit |	hit := false.	[hit := true] unless: true.	self deny: hit</body><body package="SuffixConditionals-Tests">returnsBlockValueIfConditionIsTrue	&lt;test&gt;	self assert: ([42] if: true) = 42</body><body package="SuffixConditionals-Tests">returnsBlockValueUnlessConditionIsFalse	&lt;test&gt;	self assert: ([42] unless: false) = 42</body><body package="SuffixConditionals-Tests">returnsFirstBlockValueIfConditionIsTrue	&lt;test&gt;	self assert: ([42] if: true else: [nil]) = 42</body><body package="SuffixConditionals-Tests">returnsNilIfConditionIsFalse	&lt;test&gt;	self assert: ([42] if: false) isNil</body><body package="SuffixConditionals-Tests">returnsNilUnlessConditionIsTrue	&lt;test&gt;	self assert: ([42] unless: true) isNil</body><body package="SuffixConditionals-Tests">returnsSecondBlockValueIfConditionIsFalse	&lt;test&gt;	self assert: ([nil] if: false else: [42]) = 42</body></methods><methods><class-id>SuffixConditionals.SuffixConditionalOptimizationTest</class-id> <category>tests</category><body package="SuffixConditionals-Tests">optimizationMacrosRegistered	&lt;test&gt;	self		assert: (MessageNode.MacroSelectors includesKey: #if:else:);		assert: (MessageNode.MacroSelectors includesKey: #if:);		assert: (MessageNode.MacroSelectors includesKey: #unless:)</body></methods><do-it>"Imported Classes:"</do-it><do-it>self error: 'Attempting to file-in parcel imports.  Choose terminate or close'</do-it><class><name>TestCase</name><environment>SUnit</environment><super>Core.Object</super><private>false</private><indexed-type>none</indexed-type><inst-vars>testSelector </inst-vars><class-inst-vars></class-inst-vars><imports></imports><category>SUnit</category><attributes><package>SUnitToo</package></attributes></class></st-source>